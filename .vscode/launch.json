{
  "version": "1.0.0",
  "configurations": [
    {
      "name": "Launch Program",
      "program": "${workspaceFolder}/app/index.ts",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register/transpile-only"],
      "envFile": "${workspaceFolder}/.env"
    }
    
  ]
}